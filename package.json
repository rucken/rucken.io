  "scripts": {
    "ng": "ng",
    "start": "node dist/server",
    "start:client": "ng serve",
    "start:dev": "node serve-script",
    "start:prod": "cross-env NODE_ENV=production node dist/server",
    "test": "npm run build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors --mode development",
    "build": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client": "ng build --prod",
    "build:client-and-server-bundles": "ng build --prod && ng run rucken:server:production && ls",
    "thumb": "./node_modules/.bin/ts-node ./scripts/create-thumb.ts"
  },
